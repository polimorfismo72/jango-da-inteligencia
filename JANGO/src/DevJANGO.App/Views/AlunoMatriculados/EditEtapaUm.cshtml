@model DevJANGO.App.ViewModels.AlunoMatriculadoViewModel

@{
    ViewData["Title"] = "Matricula do(a) - ";
}

<div class="container">
    <h3>@ViewData["Title"] @Model.Nome</h3>
    <hr />
    <div class="row">
        <form enctype="multipart/form-data" asp-action="EditEtapaUm">
            <vc:Summary></vc:Summary>
 
            <partial name="_EditarMatricula" />

            <div>
                &nbsp;
            </div>

            <div class="form-group">
                <input type="submit" value="Matricular" class="btn btn-primary" />
               
              @*   <a class="btn btn-warning" type="submit" asp-controller="AlunoMatriculados" asp-action="IndexMatriculaPendenteEtapaUm">
                    <i>Voltar</i>
                </a> *@
                <a class="btn btn-info" href="javascript:window.history.back();">Voltar</a>
            </div>
        </form>
    </div>
</div>

<br />

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    <script>
        $(document).ready(function () {
            SetVaga();
        });
    </script>
}
