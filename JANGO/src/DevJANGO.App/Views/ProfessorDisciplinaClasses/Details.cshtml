@model DevJANGO.App.ViewModels.ProfessorDisciplinaClasseViewModel

@{
    ViewData["Title"] = "Detalhes do Professor";
}

 
<br />
<div class="container">
    <h1>Professor(a) - @Model.Professor</h1>
    <hr />
<div>
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.NomeDisciplina)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NomeDisciplina)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.NomeClasse)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NomeClasse)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.AnoLetivo)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.AnoLetivo)
        </dd>
    
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Professor.BI)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Disciplina)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Disciplina.NomeDisciplina)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Classe)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Classe.Nome)
        </dd>
    </dl>
</div>
<div>
        <a class="btn btn-dark" asp-controller="ProfessorDisciplinaClassees" asp-action="Edit" asp-route-id="@Model.Id">Editar</a>
        <a class="btn btn-info" href="javascript:window.history.back();">Voltar</a>
    @* <a asp-action="Index">Back to List</a> *@
</div>
</div>
