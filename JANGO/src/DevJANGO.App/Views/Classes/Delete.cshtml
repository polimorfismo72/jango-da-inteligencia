@model DevJANGO.App.ViewModels.ClasseViewModel

@{
    ViewData["Title"] = "Delete";
}
<div class="container">
    <h1>Delete</h1>

    <h3>Tem certeza que quer apagar a @Html.DisplayFor(model => model.Nome) classe?</h3>
    <div>

        <form asp-action="Delete">
            <input type="hidden" asp-for="Id" />
            <input type="submit" value="Sim" class="btn btn-danger" />
            <a class="btn btn-info" href="javascript:window.history.back();">Não</a> 
        </form>
    </div>
</div>

